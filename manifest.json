{
  "name": "mercadopago-app",
  "vendor": "mercadopago",
  "version": "4.19.0",
  "title": "Mercado Pago Payment APP",
  "description": "Application that renders a modal web form for Mercado Pago checkout",
  "defaultLocale": "en",
  "mustUpdateAt": "2019-01-08",
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexpayments.com.br",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexpayments.com.br",
        "path": "/*"
      }
    },
    {
      "name": "MakePayments"
    },
    {
      "name": "ManageStore"
    },
    {
      "name": "ManageInfrastructure"
    },
    {
      "name": "NotifyPayments"
    },
    {
      "name": "ViewPayments"
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    }
  ],
  "dependencies": {
    "vtex.docs-graphql": "0.x",
    "vtex.render-runtime": "7.x",
    "vtex.render-server": "7.x"
  },
  "billingOptions": {
    "free": true,
    "type": "free",
    "availableCountries": [
      "*"
    ]
  },
  "builders": {
    "react": "2.x",
    "pages": "0.x",
    "messages": "0.x",
    "docs": "0.x",
    "node": "6.x"
  },
  "scripts": {
    "postreleasy": "vtex publish --public"
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema",
  "_buildFeatures": {
    "vtex.docs-graphql": [
      "build.json",
      "build.json"
    ],
    "vtex.render-server": [
      "build.json"
    ],
    "vtex.pages-graphql": [
      "build.json"
    ]
  }
}
